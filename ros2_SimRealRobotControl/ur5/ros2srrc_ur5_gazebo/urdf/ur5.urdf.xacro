      </inertial>
    </link>


    <joint name="LINE" type="fixed">
      <parent link="world" />
      <child link = "link_0" />
      <origin xyz="0.45 5.05 0.00" rpy="0.0 0.0 -1.5707" />
    </joint>

    <joint name="MODEL" type="fixed">
      <parent link="world" />
      <child link = "link_1" />
      <origin xyz="-0.2 3.95 0.00" rpy="0.0 0.0 0.0" />
    </joint>

    <joint name="contain" type="fixed">
