        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <mesh filename="file://$(find ros2srrc_ur5_gazebo)/meshes/ur5_CranfieldCell/XVC4B4.dae" scale="1 1 1" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <mesh filename="file://$(find ros2srrc_ur5_gazebo)/meshes/ur5_CranfieldCell/XVC4B4.dae" scale="1 1 1" />
