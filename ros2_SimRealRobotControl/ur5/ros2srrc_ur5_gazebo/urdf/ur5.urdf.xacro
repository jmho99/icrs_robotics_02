    </link>
   
    <link name='ur5_EB'>
      <visual>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <mesh filename="file://$(find ros2srrc_ur5_gazebo)/meshes/ur5_CranfieldCell/UR5emergencybutton.dae" scale="1 1 1" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <mesh filename="file://$(find ros2srrc_ur5_gazebo)/meshes/ur5_CranfieldCell/UR5emergencybutton.dae" scale="1 1 1" />
        </geometry>
      </collision>
      <inertial>
        <mass value="1.0"/>
        <inertia ixx="500.0" ixy="500.0" ixz="500.0" iyy="500.0" iyz="500.0" izz="500.0" />
      </inertial>
    </link>
    <link name='ur5_B'>
      <visual>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <mesh filename="file://$(find ros2srrc_ur5_gazebo)/meshes/ur5_CranfieldCell/UR5button.dae" scale="1 1 1" />
